# Configurações da API
EXPO_PUBLIC_API_URL=https://api.terreiro-app.com/v1

# Chaves de API (substitua pelos valores reais)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Configurações de Autenticação
EXPO_PUBLIC_AUTH0_DOMAIN=your_auth0_domain
EXPO_PUBLIC_AUTH0_CLIENT_ID=your_auth0_client_id
EXPO_PUBLIC_AUTH0_AUDIENCE=your_auth0_audience

# Configurações de Ambiente
NODE_ENV=development
EXPO_PUBLIC_ENV=development

# Configurações de Depuração
EXPO_DEBUG=true

# Configurações de Log
EXPO_PUBLIC_ENABLE_LOGGING=true
EXPO_PUBLIC_LOG_LEVEL=debug
